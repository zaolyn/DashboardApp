<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Faza.openhouse">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.Faza.openhouse.alarm"
            android:screenOrientation="portrait"/>
        <activity android:name="com.Faza.openhouse.kalendar"
            android:screenOrientation="portrait"/>
<!--        <activity android:name=".quiz"-->
<!--            android:screenOrientation="portrait"/>-->
        <activity android:name="com.Faza.openhouse.kalkulator"
            android:screenOrientation="portrait"/>
        <activity android:name="com.Faza.openhouse.stopwatcAct"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Faza.openhouse.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Faza.openhouse.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
<!--        <activity android:name=".hasilquiz" />-->
    </application>

</manifest>